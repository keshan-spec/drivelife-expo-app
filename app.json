{
  "expo": {
    "name": "DriveLife",
    "slug": "carcal-v1",
    "scheme": "carcalendar",
    "version": "3.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "androidStatusBar": {
      "backgroundColor": "#b89855",
      "barStyle": "default",
      "hidden": false
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.carcalendar",
      "buildNumber": "3.1.1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow you to take photos to create posts.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to allow you to record audio to create posts.",
        "NSLocationWhenInUseUsageDescription": "This app uses accurate location data to allow users to browse and search for events/venues by proximity.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses accurate location data to allow users to browse and search for events/venues by proximity.",
        "NSLocationAlwaysUsageDescription": "This app uses accurate location data to allow users to browse and search for events/venues by proximity.",
        "NSPhotoLibraryUsageDescription": "This app uses photo library permissions to access images and videos for posts.",
        "NSPhotoLibraryAddUsageDescription": "This app uses photo library permissions to save images and videos.",
        "NSFileProviderUsageDescription": "This app uses file provider permissions to access files for posts.",
        "NSAppleMusicUsageDescription": "This app requires access to Apple Music to access your library for posts."
      },
      "icon": "./assets/ios/appstore.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/play_store_512.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE_DATA_SYNC"
      ],
      "package": "com.app.carcalendar",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "phpstack-889362-4370795.cloudwaysapps.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/web/favicon.ico"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-compressor"
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/ic_stat_onesignal_default.png",
          "color": "#b89855",
          "sounds": [
            "./assets/custom_sound.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.app.carcalendar.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.app.carcalendar.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "14e276c1-3f3c-426f-9aa7-48c93153527f"
      },
      "onesignal": {
        "app_id": "4d73ca20-1998-415b-811a-5a5edb082f47"
      }
    }
  }
}